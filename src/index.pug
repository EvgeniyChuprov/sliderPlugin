include blocks/settings-panel/settings-panel

doctype html
html
  head
    meta(charset = 'utf-8')
    meta(name='viewport', content="initial-scale=1.0, width=device-width, shrink-to-fit=no")
    link( rel="apple-touch-icon" sizes="180x180" href=require('./favicons/apple-touch-icon.png'))
    link( rel="icon" type="image/png" sizes="32x32" href=require('./favicons/favicon-32x32.png'))
    link( rel="icon" type="image/png" sizes="16x16" href=require('./favicons/favicon-16x16.png'))
    link( rel="manifest" href="favicons/site.webmanifest")
    link( rel="mask-icon" href=require('./favicons/safari-pinned-tab.svg') color="#5bbad5")
    meta( name="msapplication-TileColor" content="#2d89ef")
    meta( name="theme-color" content="#ffffff")
    title slider
  body
    +settings-panel({idTable:'firstBlock', idSlider:'firstSlider', tooltip: false, twoSlider:true, vertical:false})
    +settings-panel({idTable:'secondBlock', idSlider:'secondSlider', tooltip: true, twoSlider:true, vertical:false})
    +settings-panel({idTable:'thirdBlock', idSlider:'thirdSlider', tooltip: true, twoSlider:false, vertical:false})
    +settings-panel({idTable:'fourthBlock', idSlider:'fourthSlider', tooltip: true, twoSlider:true, vertical:true})
